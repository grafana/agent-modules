// all combines all scrubbing components.
declare "all" {
	argument "forward_to" {
		// comment = "Must be a list(LogsReceiver) where collected logs should be forwarded to"
		optional = false
	}

	export "process" {
		value = json_empties.default.process
	}

	json_empties "default" {
		forward_to = [json_nulls.default.process.receiver]
	}

	json_nulls "default" {
		forward_to = argument.forward_to.value
	}
}
